<?xml version="1.0" encoding="UTF-8"?>
<configuration monitorInterval="60">
  <appenders>
    <console name="stdout" target="SYSTEM_OUT">
      <patternLayout pattern="[%10t] %d{ABSOLUTE} %5p %c{1}:%L - %m%n" />
    </console>
  </appenders>

  <loggers>
    <logger name="com.outbrain.BootCampOmlevi" level="DEBUG" />
    <root level="INFO">
      <appenderRef ref="stdout" />
    </root>
  </loggers>
</configuration>
