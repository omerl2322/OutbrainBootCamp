<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
  <import resource="classpath:com/outbrain/properties/applicationContext-properties.xml"/>

  <bean id="greetingMessage" class="java.lang.String">
    <constructor-arg value="This is a greeting injected message: works with env: ${com.outbrain.environment}"/>
  </bean>
</beans>
